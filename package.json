{
	"name": "line-filter",
	"displayName": "line-filter",
	"description": "filter lines in an active file by command palette",
	"icon": "images/icon.jpeg",
	"version": "0.0.8",
	"publisher": "stucci",
	"repository": {
		"type": "git",
		"url": "https://github.com/stucci/line-filter.git"
	},
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.lineFilter"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.lineFilter",
				"title": "Line Filter"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.13.1",
		"@types/vscode": "^1.39.0",
		"glob": "^7.1.6",
		"mocha": "^10.8.2",
		"typescript": "^3.8.3",
		"tslint": "^6.1.1",
		"vscode-test": "^1.3.0"
	}
}
